{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "anhskohbo/no-captcha": "^3.0",
        "cmgmyr/messenger": "^2.21",
        "coderello/laraflash": "^4.0",
        "deepskylog/laravel-astronomy-library": "^4.0",
        "deepskylog/laravel-gettext": "^7.3",
        "dompdf/dompdf": "^0.8.3",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^7.0",
        "janbarasek/php-moon-phase": "^2.0",
        "laravel/framework": "^8.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "laravelcollective/html": "^6.0",
        "livewire/livewire": "^2.0",
        "monarobase/country-list": "^3.0",
        "muhamadrezaar/highcharts": "^2.0",
        "petercoles/multilingual-language-list": "^1.15",
        "spatie/laravel-cookie-consent": "^2.8",
        "spatie/laravel-medialibrary": "^8.0.0",
        "spatie/laravel-translatable": "^4.3",
        "spatie/pdf-to-image": "^2.0",
        "yajra/laravel-datatables": "^1.0",
        "yajra/laravel-datatables-oracle": "~9.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laravel/telescope": "^4.0",
        "mockery/mockery": "^1.0",
        "phpunit/php-code-coverage": "^9.0",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": {
        "local": {
            "type": "path",
            "url": "../laravel-astronomy-library"
        }
    }
}
